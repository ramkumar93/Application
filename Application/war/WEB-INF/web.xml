<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>Personal</servlet-name>
		<servlet-class>com.controller.ApplicationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Personal</servlet-name>
		<url-pattern>/personal</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Contact</servlet-name>
 		<servlet-class>com.controller.ContactServlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Gokulgokul</servlet-name>
 		<servlet-class>com.controller.GokulgokulServlet</servlet-class> 
	</servlet>
	<servlet-mapping>
		<servlet-name>gokul</servlet-name>
		<url-pattern>/gokul</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
